@model IEnumerable<TestApplication.Areas.Identity.Data.Quiz>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="sweetalert2.all.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css">


<script type="text/javascript">
    $(document).ready(function () {
        $(".btnalertMessage").click(function () {
            swal("Privet")
        });

    });

</script>




@{
    foreach (var item in Model)
    {
        <div class="card" style="width: 18rem;">
            <img class="card-img-top" src="@item.QuizImg" alt="Card image cap">
            <div class="card-body">
                <h5 class="card-title">@item.QuizName</h5>
                <p class="card-text">@item.Description</p>
                <a href="#" class="btn btn-primary btnalertMessage">Start</a>
            </div>
        </div>
    }
}
